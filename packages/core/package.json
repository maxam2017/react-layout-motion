{
  "name": "react-layout-motion",
  "type": "module",
  "version": "0.0.0",
  "private": false,
  "description": "React layout animation library (coming soon)",
  "author": "maxam2017",
  "license": "MIT",
  "homepage": "https://github.com/maxam2017/react-layout-motion#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxam2017/react-layout-motion.git"
  },
  "bugs": {
